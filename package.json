{
  "name": "auckland",
  "version": "0.1.0-alpha",
  "description": "Auckland is a minimalist UI library for Vue 3 applications.",
  "main": "lib/auckland.common.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:playground",
    "build:playground": "vite build",
    "build:lib": "vue-cli-service build --target lib --name auckland src/index.ts --dest lib",
    "dev": "vite",
    "lint": "eslint ./src --ext .js,.ts,.vue",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CaptainYouz/auckland"
  },
  "dependencies": {
    "marked": "^1.2.0",
    "vue": "^3.0.0-rc.1",
    "vue-router": "^4.0.0-beta.11"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-rc.1",
    "@vue/test-utils": "^2.0.0-beta.6",
    "babel-jest": "^26.3.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "husky": "^5.0.0-alpha.6",
    "jest": "^26.4.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "vite": "^1.0.0-rc.1",
    "vue-jest": "^5.0.0-alpha.4"
  }
}